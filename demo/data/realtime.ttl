@prefix : <http://signalkg.visualmodel.org/demo#> .
@prefix asset: <https://w3id.org/rec/asset/> .
@prefix building: <https://w3id.org/rec/building/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skg: <http://signalkg.visualmodel.org/skg#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:attacker
  a skg:Entity ;
  skg:prExists 0.5 ;
  skg:performs :movement ;
  skg:performs :carryCellPhone ;
  skg:performs :carryWeaponKnife ;
.

:student
  a skg:Entity ;
  skg:prExists 0.5 ;
  skg:performs :movement ;
  skg:performs :carryCellPhone ;
  skg:performs :carryWeaponKnife ;
  skg:performs :carryFork ;
.

:movement
  a skg:Action ;
  skg:actsOn building:DiningRoom ;
  skg:prOccurs 0.5 ;
  skg:creates :person ;
.
:carryCellPhone
  a skg:Action ;
  skg:actsOn building:DiningRoom ;
  skg:prOccurs 0.5 ;
  skg:creates :cellphone ;
.
:carryWeaponKnife
  a skg:Action ;
  skg:actsOn building:DiningRoom ;
  skg:prOccurs 0.5 ;
  skg:creates :knife ;
.
:carryFork
  a skg:Action ;
  skg:actsOn building:DiningRoom ;
  skg:prOccurs 0.5 ;
  skg:creates :fork ;
.
:person
  a skg:Signal ;
  skos:broader :visibleObject ;
.
:cellphone
  a skg:Signal ;
  skos:broader :visibleObject ;
.
:knife
  a skg:Signal ;
  skos:broader :visibleObject ;
.
:fork
  a skg:Signal ;
  skos:broader :visibleObject ;
.
:visibleObject
  a skg:Signal ;
  skg:attenuatedBy skg:Wall ;
  skg:fallOff skg:inverseLinear ;
.
:yolo
  a skg:ClassifierModel ;
  skg:sensitivity 0.9 ;
.